{
  "name": "YAMATO Restaurant Map",
  "description": "Yamato Lab. of Lenovo Japan shares restaurant map",
  "version": "2.5.0",
  "manifest_version": 2, 
  //"app": {
  //  "launch": {
  //    "local_path": "main.html"
  //  }
  //},
  "icons": {
    "48": "rc/len_48.gif",
    "128": "rc/len_128.gif"
  },
  "background": {"scripts": ["bg.js"]},
  "browser_action": {
    "default_icon": {
      "19": "rc/len_19.gif",
      "38": "rc/len_38.gif"
    },
    "default_title": "YAMATO Restaurant Map"
  },
  "permissions": [
    "http://tab.do/",
    "https://tab.do/"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://*.google.com https://*.googleapis.com https://*.gstatic.com; object-src 'self'"
}
